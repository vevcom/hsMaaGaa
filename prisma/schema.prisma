
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Call {
    distance Int
    timestamp DateTime @id @default(now())
    rfid String
    user User @relation(fields: [rfid], references: [rfid])
}

model User {
    username String
    firstName String 
    lastName String 
    rfid String @id
    calls Call[]

}