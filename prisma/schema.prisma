
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Call {
    distance Int
    timestamp DateTime @id @default(now())
    rfid String
    user User @relation(fields: [rfid], references: [rfid])
}

model User {
    username String
    firstname String 
    lastname String 
    rfid String @id
    calls Call[]

}